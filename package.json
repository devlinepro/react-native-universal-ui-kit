{
  "name": "@devline/react-universal-ui-kit",
  "version": "0.0.1",
  "private": true,
  "main": "src/app.tsx",
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "engineStrict": true,
  "scripts": {
    "prepare": "husky install && cp -n .env.example .env",
    "start": "expo start",
    "start-tunnel": "expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devlinepro/react-native-universal-ui-kit"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "files": [
    "lib/components",
    "lib/theme",
    "readme.md"
  ],
  "prettier": {
    "useTabs": true,
    "trailingComma": "es5",
    "printWidth": 120
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@expo/metro-runtime": "^2.2.14",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@types/react": "~18.2.14",
    "expo": "~49.0.21",
    "expo-image": "~1.5.1",
    "lodash": "^4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-web": "~0.19.10",
    "react-scrollbars-custom": "^4.1.1",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^18.4.1",
    "@commitlint/config-conventional": "^18.4.0",
    "@tsconfig/react-native": "^3.0.2",
    "@types/lodash": "^4.14.201",
    "husky": "^8.0.3",
    "prettier": "2.8.8"
  }
}
